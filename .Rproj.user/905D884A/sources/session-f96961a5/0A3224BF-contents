Package: musquashMPA
Type: Package
Title: Analysis Tools for Musquash Marine Protected Area Data
Version: 0.1.0
Authors@R: c(
    person("Peike", "Cong", email = "pk376328@dal.ca", role = "aut"),
    person("Cameron", "Moffatt", email = "cm853581@dal.ca", role = "aut"),
